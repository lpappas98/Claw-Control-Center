{
  "name": "claw-control-center",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "bridge": "node bridge/server.mjs",
    "beats": "node bridge/slot-heartbeats.mjs",
    "test": "node --test 'scripts/**/*.test.mjs' bridge/instanceDiscovery.test.mjs bridge/taskTemplates.test.mjs bridge/taskTemplates.integration.test.mjs bridge/cronParser.test.mjs bridge/routines.test.mjs bridge/routineExecutor.test.mjs bridge/routines-api.test.mjs bridge/telegramIntegration.test.mjs",
    "test:phase2": "node --test 'scripts/**/*.test.mjs' bridge/instanceDiscovery.test.mjs",
    "test:phase3": "node --experimental-test-loader=./node_modules/@testing-library/react src/**/*.test.tsx src/services/api.test.ts 2>/dev/null || echo 'Phase 3 tests require jsdom support'",
    "test:e2e": "node e2e/run-e2e-tests.mjs",
    "test:e2e:api": "node --test e2e/task-tests.mjs",
    "test:e2e:playwright": "npx -y @playwright/test test",
    "test:e2e:ui": "npx -y @playwright/test test --ui",
    "test:e2e:debug": "npx -y @playwright/test test --debug",
    "test:e2e:headed": "npx -y @playwright/test test --headed",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^8.0.0",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-switch": "^1.2.6",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "react": "^19.2.0",
    "react-day-picker": "^9.13.2",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.71.1",
    "react-router-dom": "^7.13.0",
    "winston": "^3.19.0",
    "ws": "^8.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.58.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/ui": "^4.0.18",
    "autoprefixer": "^10.4.24",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^28.0.0",
    "lucide-react": "^0.564.0",
    "postcss": "^8.5.6",
    "supertest": "^7.2.2",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.9",
    "typescript-eslint": "^8.48.0",
